        "bson": "^4.6.5",
        "dialogs": "^2.0.1",
        "electron": "^18.3.7",
        "fs-extra": "^10.1.0",
        "keytar": "^7.9.0",
        "lokijs": "^1.5.12",
        "mime": "^3.0.0",
